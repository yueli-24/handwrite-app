{
  "builds": [
    { "src": "api/python/*.py", "use": "@vercel/python" },
    { "src": "package.json", "use": "@vercel/next" }
  ],
  "routes": [
    { "src": "/api/generate", "dest": "/api/python/generate.py" },
    { "src": "/(.*)", "dest": "/$1" }
  ],
  "env": {
    "PYTHON_ENABLED": "true"
  }
}
